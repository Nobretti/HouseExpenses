# ===========================================
# Supabase Database Configuration
# ===========================================
# Get from: Supabase Dashboard > Project Settings > Database
# Use the "Connection string" > "JDBC" format

DATABASE_URL=jdbc:postgresql://db.YOUR_PROJECT_ID.supabase.co:5432/postgres
DB_USERNAME=postgres
DB_PASSWORD=YOUR_DATABASE_PASSWORD

# ===========================================
# Supabase API Configuration
# ===========================================
# Get from: Supabase Dashboard > Project Settings > API

SUPABASE_URL=https://YOUR_PROJECT_ID.supabase.co
SUPABASE_ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...
SUPABASE_SERVICE_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...

# ===========================================
# JWT Configuration
# ===========================================
# Generate a secure 256-bit key for production

JWT_SECRET=your-super-secret-jwt-key-minimum-32-characters-long
